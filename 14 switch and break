//switch and break
#include<iostream>
using namespace std;
int main()
{
    int var;
    cin>>var;
    switch(var)
    {
        case 1:
        cout<<"one"<<endl;
        break; //break didn't execute all case coming under the executed case
        case 2:
        cout<<"two"<<endl;
        break;
        case 3:
        cout<<"three"<<endl;
        break;
        case 4:
        cout<<"four"<<endl;
        break;
        default:
        cout<<"invalid"<<endl;
    }
    return 0;
}

