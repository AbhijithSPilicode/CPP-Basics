#include<iostream>
using namespace std;
int arr[10];
int main()
{
    cout<<"enter array elements"<<endl;
    for(int i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    
    cout<<"Array elements are:"<<endl;
    for(int i=0;i<5;i++)
    {
        if(arr[i]>arr[0])
        {
            arr[0]=arr[i];
        }
    }
    cout<<"biggest element in the array is "<<arr[0];
    return 0;
}

