//2D array

#include<iostream>
using namespace std;

int main()
{
  int arr[3][2]={{12,23},{34,45},{56,67}};
      
      cout<<arr[1][1]<<"\n";
      
      for(int i=0;i<3;i++)
      {
          for(int j=0;j<2;j++)
          {
            cout<<arr[i][j]<<"\t";  
          }
          cout<<"\n";
      }
      return 0;
}
