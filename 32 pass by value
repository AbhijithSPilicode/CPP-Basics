//pass by value
#include<iostream>
using namespace std;
void test(int x);
int main()
{
 int x=12;
 cout<<"value of x before calling function "<<x<<endl;
 test(x);
 cout<<"the value of x after calling function "<<x<<endl;
 return 0;
}

void test(int x)
{
    x=100;
    cout<<"value of x inside function "<<x<<endl;
}

