//else-if ladder
#include<iostream>
using namespace std;

int main()
{
  int a;
  cout<<"enter a number"<<endl;
  cin>>a;
  
  if(a%10==0)
  {
      cout<<"divisible by both 5 and 2"<<endl;
  }
  else if(a%5==0)
  {
      cout<<"divisible by five"<<endl;
  }
  else if(a%2==0)
  {
      cout<<"divisible by two"<<endl;
  }
  else
  {
      cout<<"divisible by not 5 or 2"<<endl;
  }
  return 0;
}
